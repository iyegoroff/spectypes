{
  "name": "spectypes",
  "version": "1.0.6",
  "private": false,
  "description": "Fast, compiled, eval-free data validator/transformer",
  "sideEffects": false,
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/esm/index.js",
  "types": "./dist/dts/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/cjs/index.cjs",
      "import": "./dist/esm/index.js"
    },
    "./package.json": "./package.json"
  },
  "type": "module",
  "scripts": {
    "update-version": "npm version",
    "build:dts": "tsc -p node_modules/spectypes-configs/tsconfig.dts.json",
    "build:esm": "tsc -p node_modules/spectypes-configs/tsconfig.esm.json",
    "build:cjs": "build-for-cjs node_modules/spectypes-configs/tsconfig.cjs.json",
    "build": "rm -rf dist && mkdir dist && npm run build:dts && npm run build:esm && npm run build:cjs"
  },
  "keywords": [
    "validation",
    "validator"
  ],
  "files": [
    "dist",
    "src"
  ],
  "bugs": {
    "url": "https://github.com/iyegoroff/spectypes/issues"
  },
  "homepage": "https://github.com/iyegoroff/spectypes#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iyegoroff/spectypes.git"
  },
  "author": "iyegoroff <iegoroff@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "build-for-cjs": "^0.0.5",
    "spectypes-configs": "workspace:*"
  }
}
