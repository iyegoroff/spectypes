{
  "name": "jsonspec",
  "version": "0.0.12",
  "private": false,
  "description": "Fast, compiled, eval-free data validator/transformer",
  "sideEffects": false,
  "main": "src/index.ts",
  "publishConfig": {
    "main": "./dist/cjs/index.js",
    "module": "./dist/esm/index.js",
    "types": "./dist/dts/index.d.ts",
    "exports": {
      ".": {
        "require": "./dist/cjs/index.js",
        "import": "./dist/esm/index.js"
      },
      "./package.json": "./package.json"
    }
  },
  "scripts": {
    "update-version": "npm version",
    "build:dts": "tsc -p node_modules/jsonspec-configs/tsconfig.dts.json",
    "build:esm": "tsc -p node_modules/jsonspec-configs/tsconfig.esm.json",
    "build:cjs": "tsc -p node_modules/jsonspec-configs/tsconfig.cjs.json",
    "build": "rm -rf dist && mkdir dist && npm run build:dts && npm run build:esm && npm run build:cjs"
  },
  "keywords": [
    "validation",
    "validator"
  ],
  "files": [
    "dist",
    "src"
  ],
  "bugs": {
    "url": "https://github.com/iyegoroff/jsonspec/issues"
  },
  "homepage": "https://github.com/iyegoroff/jsonspec#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iyegoroff/jsonspec.git"
  },
  "author": "iyegoroff <iegoroff@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "jsonspec-configs": "workspace:*"
  }
}
