{
  "name": "babel-plugin-spectypes",
  "version": "0.0.42",
  "private": false,
  "description": "Babel plugin that compiles spectypes validators",
  "sideEffects": false,
  "main": "./dist/cjs/index.js",
  "types": "./dist/dts/index.d.ts",
  "scripts": {
    "update-version": "npm version",
    "build:dts": "tsc -p node_modules/spectypes-configs/tsconfig.dts.json",
    "build:cjs": "tsc -p node_modules/spectypes-configs/tsconfig.cjs.json",
    "build": "rm -rf dist && mkdir dist && npm run build:dts && npm run build:cjs"
  },
  "keywords": [
    "babel-plugin",
    "validation",
    "validator"
  ],
  "files": [
    "dist",
    "src"
  ],
  "bugs": {
    "url": "https://github.com/iyegoroff/spectypes/issues"
  },
  "homepage": "https://github.com/iyegoroff/spectypes#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iyegoroff/spectypes.git"
  },
  "author": "iyegoroff <iegoroff@gmail.com>",
  "license": "MIT",
  "peerDependencies": {
    "@babel/core": "*",
    "spectypes": "workspace:*"
  },
  "devDependencies": {
    "@babel/parser": "^7.17.3",
    "@types/babel__core": "^7.1.18",
    "@types/babel__generator": "^7.6.4",
    "@types/babel__traverse": "^7.14.2",
    "@types/chance": "^1.1.3",
    "@types/hash-sum": "^1.0.0",
    "@types/node": "^17.0.18",
    "@types/string-hash": "^1.1.1",
    "chance": "^1.1.8",
    "escape-string-regexp": "4.0.0",
    "fast-check": "^2.22.0",
    "hash-sum": "^2.0.0",
    "spectypes": "workspace:*",
    "spectypes-configs": "workspace:*",
    "string-hash": "^1.1.3",
    "ts-assert-unreachable": "^0.0.9",
    "ts-is-record": "^2.0.3",
    "ts-micro-dict": "^8.1.5",
    "ts-tiny-invariant": "^1.0.5"
  },
  "dependencies": {
    "js-name-escape": "^1.0.1",
    "pipe-ts": "^0.0.9",
    "ts-is-defined": "^2.0.3",
    "ts-railway": "^5.1.5"
  }
}
