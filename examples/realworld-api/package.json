{
  "name": "realworld-api",
  "version": "0.0.0",
  "license": "MIT",
  "private": true,
  "type": "module",
  "scripts": {
    "type-check": "tsc -p ./src --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "rm -rf ./build && npm run type-check && npm run build:js",
    "build:js": "babel src --out-dir build --extensions \".ts,.tsx\"",
    "start": "npm run build && node ./build/index.js"
  },
  "dependencies": {
    "fetch-ponyfill": "^7.1.0",
    "fetchmap": "^0.0.11",
    "node-fetch": "^3.2.4",
    "pipe-ts": "^0.0.9",
    "spectypes": "workspace:*",
    "ts-micro-dict": "^8.1.5",
    "ts-railway": "^6.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.10",
    "@babel/core": "^7.17.10",
    "@babel/preset-env": "^7.17.10",
    "@babel/preset-typescript": "^7.16.7",
    "babel-plugin-spectypes": "workspace:*"
  }
}
