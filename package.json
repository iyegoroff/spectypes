    "@types/babel__core": "^7.1.19",
    "@types/babel__generator": "^7.6.4",
    "@types/babel__traverse": "^7.17.1",
    "@types/chance": "^1.1.3",
    "@types/hash-sum": "^1.0.0",
    "@types/jest": "^28.1.6",
    "@types/node": "^18.6.3",
    "@types/string-hash": "^1.1.1",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "chance": "^1.1.8",
    "eslint": "^8.21.0",
