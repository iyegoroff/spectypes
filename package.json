{
  "name": "spectypes-root",
  "version": "2.1.4",
  "private": true,
  "files": [],
  "scripts": {
    "preversion": "npm version $npm_new_version -ws && git commit -am up && npm run build && npm run type-check && npm run lint && npm t && npm run type-coverage",
    "postversion": "git push && git push --tags",
    "lint": "eslint . && npm run lint -ws --if-present",
    "build": "npm run build -w packages/spectypes-plugin-core && npm run build -ws --if-present",
    "type-check": "npm run type-check -ws --if-present",
    "test": "jest",
    "patch-manifests": "glob-json 'packages/*/package.json' merge '' publishConfig && glob-json 'packages/*/package.json' del publishConfig && glob-json 'packages/*/package.json' set '+(devDependencies|dependencies|peerDependencies)/@ts-elmish/*' $npm_package_version",
    "type-coverage": "type-coverage --strict --detail --ignore-files **/dist/** --ignore-files **/blueprint-templates/**/* --ignore-catch"
  },
  "typeCoverage": {
    "atLeast": 99.9
  },
  "devDependencies": {
    "@babel/core": "^7.18.5",
    "@babel/parser": "^7.18.5",
    "@babel/runtime": "^7.18.3",
    "@swc/core": "^1.2.203",
    "@swc/jest": "^0.2.21",
    "@types/babel__core": "^7.1.19",
    "@types/babel__generator": "^7.6.4",
    "@types/babel__traverse": "^7.17.1",
    "@types/chance": "^1.1.3",
    "@types/hash-sum": "^1.0.0",
    "@types/jest": "^28.1.2",
    "@types/node": "^18.0.0",
    "@types/string-hash": "^1.1.1",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "chance": "^1.1.8",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard-with-typescript": "^21.0.1",
    "eslint-plugin-functional": "^3.7.2",
    "eslint-plugin-import": "2.24.2",
    "eslint-plugin-no-null": "^1.0.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^5.2.0",
    "fast-check": "^2.22.0",
    "glob": "^8.0.3",
    "glob-json": "^0.0.2",
    "hash-sum": "^2.0.0",
    "jest": "^28.1.1",
    "prettier": "^2.7.1",
    "string-hash": "^1.1.3",
    "stringify-parse": "^1.0.6",
    "ts-assert-unreachable": "^0.0.9",
    "ts-is-record": "^3.0.5",
    "ts-jest": "^28.0.5",
    "ts-micro-dict": "^10.0.6",
    "ts-tiny-invariant": "^2.0.4",
    "type-coverage": "^2.21.1",
    "typescript": "^4.7.4"
  },
  "workspaces": [
    "./packages/*",
    "./examples/*",
    "./benchmark"
  ]
}
